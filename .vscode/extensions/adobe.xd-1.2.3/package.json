{
	"name": "xd",
	"displayName": "Adobe XD",
	"description": "",
	"version": "1.2.3",
	"publisher": "adobe",
	"repository": "https://letsxd.com/vscode",
	"icon": "build/app-icon.png",
	"engines": {
		"vscode": "^1.47.0",
		"node": "12.18.x"
	},
	"categories": [
		"Programming Languages",
		"Snippets",
		"Themes",
		"Other",
		"Language Packs",
		"Visualization"
	],
	"activationEvents": [
		"onCommand:xd.toggle",
		"onCommand:xd.search",
		"onWebviewPanel:xd",
		"onUri",
		"*"
	],
	"main": "./build/extension-bundle.js",
	"contributes": {
		"commands": [
			{
				"command": "xd.toggle",
				"title": "Toggle Adobe XD Panel",
				"category": "XD"
			},
			{
				"command": "xd.exportLog",
				"title": "Export debug logs",
				"category": "XD"
			},
			{
				"command": "xd.search",
				"title": "Find design tokens or components",
				"category": "XD"
			}
		],
		"keybindings": [
			{
				"command": "xd.toggle",
				"key": "ctrl+alt+x",
				"mac": "cmd+alt+x"
			},
			{
				"command": "xd.search",
				"key": "ctrl+shift+x",
				"mac": "cmd+shift+x"
			}
		],
		"configuration": {
			"title": "Adobe XD",
			"properties": {
				"xd.globalEditor": {
					"type": "boolean",
					"default": false,
					"description": "To prevent accidental changes, editor permissions are disabled by default. Enable editing of all DSP packages."
				},
				"xd.lastPackage": {
					"type": "string",
					"default": "",
					"description": "URI of the last package loaded."
				},
				"xd.authorName": {
					"type": "string",
					"default": "",
					"description": "Name of the current editor that will be used to populate the author in DSP."
				}
			}
		}
	},
	"husky": {
		"hooks": {
			"pre-commit": "git-branch-is -r \"^((?!master).)*$\" || (git-branch-is master && npm run publish)"
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run publish",
		"package": "npx vsce package",
		"publish": "npm run compile && npm run test",
		"compile": "npm run --prefix shared-src build && webpack --env.production",
		"dev": "concurrently \"npm run --prefix shared-src dev\" \"webpack --env.development --watch --info-verbosity verbose\"",
		"livedev": "concurrently 'npm run --prefix shared-src dev' 'webpack-dev-server --env.development'",
		"lint": "eslint shared-src src web-src/src --ext ts",
		"test": "jest --projects ./jest.config.js web-src/jest.config.js shared-src/jest.config.js",
		"download-strings": "cd utils && npx ts-node download-strings.ts",
		"postinstall": "cd shared-src && npm i && cd ../web-src && npm i"
	},
	"devDependencies": {
		"@babel/plugin-transform-modules-commonjs": "^7.10.4",
		"@babel/preset-env": "^7.10.4",
		"@types/color": "^3.0.1",
		"@types/colors": "^1.2.1",
		"@types/findup-sync": "^2.0.2",
		"@types/fs-extra": "^9.0.1",
		"@types/get-folder-size": "^2.0.0",
		"@types/glob": "^7.1.3",
		"@types/got": "^9.6.11",
		"@types/jest": "^26.0.4",
		"@types/lunr": "^2.3.3",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.13.13",
		"@types/query-string": "^6.3.0",
		"@types/svg-parser": "^2.0.1",
		"@types/uuid": "^8.0.0",
		"@types/vscode": "^1.47.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"colors": "^1.4.0",
		"concurrently": "^5.2.0",
		"copy-webpack-plugin": "^6.1.0",
		"css-loader": "^3.6.0",
		"dotenv-webpack": "^2.0.0",
		"eslint": "^6.8.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-jsx-a11y": "^6.3.1",
		"eslint-plugin-react": "^7.20.6",
